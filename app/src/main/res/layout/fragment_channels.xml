<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/section_wrapper"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	android:layout_marginBottom="@dimen/margin_89"
	android:orientation="vertical">

	<include android:id="@+id/sim_card"
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:visibility="gone"
		layout="@layout/channel_card_grid" />

	<LinearLayout android:id="@+id/country_wrapper"
		android:layout_width="match_parent"
		android:layout_height="match_parent"
		android:orientation="vertical" />

	<include android:id="@+id/all_card"
		layout="@layout/channel_card_grid" />

	<include android:id="@+id/request_an_account"
	    layout="@layout/request_account_header" />

</LinearLayout>