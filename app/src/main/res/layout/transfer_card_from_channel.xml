<?xml version="1.0" encoding="utf-8"?>
<com.hover.stax.views.StaxCardView xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:layout_width="match_parent"
	android:layout_height="wrap_content"
	app:title="@string/fromacct_cardhead">

	<RadioGroup
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginHorizontal="@dimen/margin_3"
		android:id="@+id/fromRadioGroup"/>

	<TextView
		android:layout_width="match_parent"
		android:layout_height="wrap_content"
		android:layout_marginHorizontal="@dimen/margin_3"
		android:layout_marginBottom="@dimen/margin_8"
		android:id="@+id/add_new_account"
		android:textColor="@color/brightBlue"
		android:text="@string/link_addacct"
		android:textSize="@dimen/text_16"
		android:fontFamily="font-medium"/>

</com.hover.stax.views.StaxCardView>